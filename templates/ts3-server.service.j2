[Unit]
Description=TeamSpeak3 server
Wants=network-online.target
After=syslog.target network.target local-fs.target

[Service]
WorkingDirectory={{ teamspeak_server_install_dir }}
User={{ teamspeak_server_user }}
Group={{ teamspeak_server_user }}
ExecStart={{ teamspeak_server_bin }} license_accepted=1
Type=simple
Restart=always

[Install]
WantedBy=multi-user.target
